@model IEnumerable<Post>

<h2>Home Page</h2>

<img src="~/content/static/jason-hawke-gduUBwIc4j4-unsplash.jpg" height="300" />

<div>
    <ul>
        @foreach (var post in Model) {
            <li>
                <a asp-controller="Home" asp-action="Post" asp-route-id="@post.Id">
                    @post.Title
                </a>
            </li>
        }
    </ul>
</div>